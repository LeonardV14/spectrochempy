;loop counters

define loopcounter COUNTER
define loopcounter SCALEF
define loopcounter FACTOR1
define loopcounter FACTOR2
define loopcounter FACTOR3



;$Id: Delay.incl,v 1.10 2000/05/08 11:39:02 eng Exp $
# 7 "/u/exp/stan/nmr/lists/pp/oneone.cpj" 2



1 ze
2 1m do:f3 do:f2
  d1
  1m pl1:f1 pl12:f2
  (p1 ph1):f1
  d2
  (p2 ph2):f1
  3u
  p20:gp0
  d4 
  (p1 ph3):f1
  d2
  d2
  (p2 ph4):f1
  p20:gp0
  d4 pl13:f3
  go=2 ph31 cpd2:f2 cpd3:f3
  1m do:f3 do:f2
  wr #0
exit

ph1=0
ph2=2
ph3=0 1 2 3
ph4=2 3 0 1
ph31=0 2 0 2

;pl1: default 1H power level
;p1 : f1 channel -  90 degree high power pulse
;p2 : p2=p1-0.15u -  90 degree high power pulse
;                   use for fine adjustment for water suppression
;p20: 200us, 30% xyz
;d1 : relaxation delay; 1-5 * T1
;d2 : 100u at 600 for optimal excitation at 8.5ppm
;     d2 = (1/(2*d)), d = distance of next null (in Hz)
;d4 : 200us min.

;gpx0: 30%
;gpy0: 30%
;gpz0: 30%

;gpnam0:  SINE.20

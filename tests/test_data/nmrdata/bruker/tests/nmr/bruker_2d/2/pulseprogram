# 26 "/u/exp/stan/nmr/lists/pp/hsqc-rk-fb+3919.cpj" 2

# 1 "/u/exp/stan/nmr/lists/pp/Delay.incl" 1
;Delay.incl  -  include file for commonly used delays
;
;version 00/02/07


;general delays

define delay DELTA
